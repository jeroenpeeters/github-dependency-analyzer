                                <get src="${srcdir}/gts/temporalObjects.xsd" dest="${destdir}/gts/temporalObjects.xsd" />
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

        </plugins>

    </build>


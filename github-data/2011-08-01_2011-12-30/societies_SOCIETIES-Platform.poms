   <groupId>org.societies.android.platform.cismanager.container</groupId>
   <artifactId>ContainerSocietiesCISManager</artifactId>
   <name>ContainerPlatform Societies CISManager</name>
   <version>${release.version}</version>
   <packaging>apk</packaging>

   <developers>
      <developer>
		 <name>WP4 Client Framework Team</name>
		<!-- Name of component author-->
		 <email>http://www.ict-societies.eu</email>
		<!-- Component author e-mail-->
	  </developer>
   </developers>

	<dependencies>    
		<dependency>
	    	<groupId>com.google.android</groupId>
	        <artifactId>android</artifactId> 
	        <exclusions>
				<!-- Provided by Android -->
				<exclusion>
					<artifactId>xpp3</artifactId>
					<groupId>xpp3</groupId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
		    <groupId>org.societies.android.platform.cis</groupId>
		    <artifactId>CommunityManagerService</artifactId>
			<version>${release.version}</version>
			<type>apklib</type>
		</dependency>   
		<dependency>
			<artifactId>xpp3</artifactId>
			<groupId>xpp3</groupId>
			<scope>provided</scope>
		</dependency>
      
    </dependencies>

    <build>
        <finalName>${project.artifactId}</finalName>
        <sourceDirectory>src</sourceDirectory>
          <plugins>
              <plugin>
                  <groupId>com.jayway.maven.plugins.android.generation2</groupId>
                  <artifactId>android-maven-plugin</artifactId>

				  <configuration>
				      <emulator>
						  <avd>Nexus403</avd>
						  <wait>60000</wait>
						  <options>-no-skin</options>
				      </emulator>
                      <!-- emma start -->
					    <emmaEnable>true</emmaEnable>
					    <emmaClassFolders>${project.basedir}/target/classes</emmaClassFolders>
					    <emmaOutputMetaFile>${project.basedir}/target/emma/coverage.em</emmaOutputMetaFile>
				      <!--emma>
                          <enable>true</enable>
                          <classFolders>${project.basedir}/target/classes</classFolders>
                          <outputMetaFile>${project.basedir}/target/emma/coverage.em</outputMetaFile>
                      </emma-->
                      <!-- emma stop -->
                      <dex>
                          <noLocals>true</noLocals>
                          <!--  must be set for emma -->
                      </dex>
                  </configuration>
                  <executions>
			    	<!--execution>
				        <id>startEmulator</id>
				        <phase>initialize</phase>
				        <goals>
				            <goal>emulator-start</goal>
				        </goals>
			    	</execution-->
			    	<!--execution>
				        <id>stopEmulator</id>
				        <phase>initialize</phase>
				        <goals>
				            <goal>emulator-stop</goal>
				        </goals>
			    	</execution-->
			    	
				</executions>
				  
                  <extensions>true</extensions>
              </plugin>
          </plugins>

    </build>

</project>

	<modelVersion>4.0.0</modelVersion>
	<groupId>org.societies.api.rfid</groupId>
	<artifactId>societies-rfid-schema</artifactId>
	<version>${release.version}</version>
	<name>SOCIETIES RFID Schema API</name>
	<packaging>jar</packaging>

	<developers>
		<developer>
			<name>Eliza Papadopoulou </name>
			<organization>HWU</organization>
			<organizationUrl>http://www.hw.ac.uk/</organizationUrl>
		</developer>
	</developers>
	<parent>
		<groupId>org.societies</groupId>
		<artifactId>spring-osgi-parent</artifactId>
		<version>RELEASE</version>
	</parent>
	<dependencies>
		<dependency>
			<groupId>com.sun.xml.bind</groupId>
			<artifactId>jaxb-impl</artifactId>
			<version>2.1.8</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.simpleframework</groupId>
			<artifactId>simple-xml</artifactId>
			<version>2.6.9</version>
			<scope>compile</scope>
			<exclusions>
				<!-- StAX is not available on Android -->
				<exclusion>
					<artifactId>stax</artifactId>
					<groupId>stax</groupId>
				</exclusion>
				<exclusion>
					<artifactId>stax-api</artifactId>
					<groupId>stax</groupId>
				</exclusion>
				<!-- Provided by Android -->
				<exclusion>
					<artifactId>xpp3</artifactId>
					<groupId>xpp3</groupId>
				</exclusion>
			</exclusions>
		</dependency> 
		<dependency>
			<groupId>org.societies.simple.converters</groupId>
			<artifactId>SimpleXMLConvertersBasic</artifactId>
			<version>${release.version}</version>
			<scope>compile</scope>
		</dependency>
	</dependencies>
	<build>
		<sourceDirectory>src/main/resources</sourceDirectory>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<excludes>
					<exclude>*.xjb</exclude>
					<!-- <exclude>*.xsd</exclude> -->
				</excludes>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<!-- 1) generate-sources jaxb2:generate 2) process-sources jaxb2simple:convert 
				3) compile maven-compiler-plugin -->
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<version>0.8.0</version>
				<executions>
					<execution>
						<id>global-generate</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<schemaExcludes>
								<exclude>dataforms.xsd</exclude>
								<exclude>pubsub.xsd</exclude>
								<exclude>pubsub#errors.xsd</exclude>
								<exclude>pubsub#owner.xsd</exclude>
								<exclude>pubsub#event.xsd</exclude>
							</schemaExcludes>
							<args>
								<arg>-Xcollection-setter-injector</arg>
								<arg>-Xdefault-value</arg>
							</args>
							<plugins>
				                <plugin>
				                    <groupId>org.jvnet.jaxb2_commons</groupId>
				                    <artifactId>jaxb2-default-value</artifactId>
				                    <version>1.1</version>
				                </plugin>
				            </plugins>
						</configuration>
					</execution>
					<execution>
						<id>dataforms-generate</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<schemaIncludes>
								<include>dataforms.xsd</include>
							</schemaIncludes>
							<generateDirectory>${project.build.directory}/generated-sources/dataforms</generateDirectory>
						</configuration>
					</execution>
					<execution>
						<id>pubsub-generate</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<schemaIncludes>
								<include>pubsub.xsd</include>
							</schemaIncludes>
							<bindingDirectory>target/generated-sources/dataforms/META-INF</bindingDirectory>
							<bindingIncludes>
								<include>*.episode</include>
							</bindingIncludes>
							<generateDirectory>${project.build.directory}/generated-sources/pubsub</generateDirectory>
						</configuration>
					</execution>
					<execution>
						<id>errors-generate</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<schemaIncludes>
								<include>pubsub#errors.xsd</include>
							</schemaIncludes>
							<generatePackage>org.jabber.protocol.pubsub.errors</generatePackage>
							<generateDirectory>${project.build.directory}/generated-sources/errors</generateDirectory>
						</configuration>
					</execution>
					<execution>
						<id>owner-generate</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<schemaIncludes>
								<include>pubsub#owner.xsd</include>
							</schemaIncludes>
							<bindingDirectory>target/generated-sources/dataforms/META-INF</bindingDirectory>
							<bindingIncludes>
								<include>*.episode</include>
							</bindingIncludes>
							<generatePackage>org.jabber.protocol.pubsub.owner</generatePackage>
							<generateDirectory>${project.build.directory}/generated-sources/owner</generateDirectory>
						</configuration>
					</execution>
					<execution>
						<id>event-generate</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<schemaIncludes>
								<include>pubsub#event.xsd</include>
							</schemaIncludes>
							<bindingDirectory>target/generated-sources/dataforms/META-INF</bindingDirectory>
							<bindingIncludes>
								<include>*.episode</include>
							</bindingIncludes>
							<generatePackage>org.jabber.protocol.pubsub.event</generatePackage>
							<generateDirectory>${project.build.directory}/generated-sources/event</generateDirectory>
						</configuration>
					</execution>
				</executions>
				<configuration>
					<strict>true</strict>
					<verbose>false</verbose>
					<extension>true</extension>
					<readOnly>yes</readOnly>
					<removeOldOutput>true</removeOldOutput>
				</configuration>
				<dependencies>
					<dependency>
						<groupId>net.java.dev.vcc.thirdparty</groupId>
						<artifactId>collection-setter-injector</artifactId>
						<version>0.5.0-1</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>org.societies.maven</groupId>
				<artifactId>jaxb2simple-maven-plugin</artifactId>
				<version>1.0</version>
				<executions>
					<execution>
						<phase>process-sources</phase>
						<goals>
							<goal>convert</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<!-- <folderInputDirectory>${project.build.directory}/generated-sources/</folderInputDirectory> -->
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<skipTests>true</skipTests>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.5</version>
				<configuration>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-site-plugin</artifactId>
				<version>3.0</version>
				<configuration>
					<reportPlugins>
						<plugin>
							<groupId>org.apache.maven.plugins</groupId>
							<artifactId>maven-javadoc-plugin</artifactId>
							<version>2.8.1</version>
						</plugin>
					</reportPlugins>
				</configuration>
			</plugin>
		</plugins>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											org.societies.maven
										</groupId>
										<artifactId>
											jaxb2simple-maven-plugin
										</artifactId>
										<versionRange>
											[1.0-SNAPSHOT,)
										</versionRange>
										<goals>
											<goal>convert</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
</project>

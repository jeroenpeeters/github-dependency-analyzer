        </property>
      </activation>
      <build>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-gpg-plugin</artifactId>
            <executions>
              <execution>
                <id>sign-artifacts</id>
                <phase>verify</phase>
                <goals>
                  <goal>sign</goal>
                </goals>
              </execution>
            </executions>
            <configuration>
              <keyname>Sean Owen (Cloudera) &lt;sowen@cloudera.com&gt;</keyname>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
    <!-- Separate profile for Hadoop 2.2.0 and later because it requires a recompile,
         due to protobuf 2.4-to-2.5 difference -->
    <profile>
      <id>hadoop220+</id>
      <properties>
        <hadoop.version>2.2.0</hadoop.version>
        <!--<crunch.version>0.9.0-hadoop2</crunch.version>-->
      </properties>
    </profile>
    <!-- Separate profile for 2.3.0 because of thinks like enum difference in JobContext? -->
    <profile>
      <id>hadoop230+</id>
      <properties>
        <hadoop.version>2.3.0</hadoop.version>
        <!--<crunch.version>0.9.0-hadoop2</crunch.version>-->
      </properties>
    </profile>
  </profiles>

</project>

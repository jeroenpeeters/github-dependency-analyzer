  <name>Java HTTP cache :: Resolvers :: Commons HttpClient</name>
  <description>Responseresolver using Commons HttpClient</description>

  <dependencies>
    <dependency>
      <groupId>commons-httpclient</groupId>
      <artifactId>commons-httpclient</artifactId>
      <version>3.1</version>

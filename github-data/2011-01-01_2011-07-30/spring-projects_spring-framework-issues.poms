                </includes>
                <excludes>
                    <exclude>**/Abstract*.java</exclude>
                    <exclude>**/*IntegrationTests.java</exclude>
                </excludes>
                <junitArtifactName>junit:junit</junitArtifactName>
            </configuration>
        </plugin>
    </plugins>
  </build>


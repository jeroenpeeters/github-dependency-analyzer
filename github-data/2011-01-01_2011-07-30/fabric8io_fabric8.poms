                  <argument>io.fabric8.gateway.handlers.detecting.protocol.openwire.codec</argument>
                  <argument>io.fabric8.gateway.handlers.detecting.protocol.openwire.command</argument>
                </arguments>
              </configuration>
            </plugin>
          </plugins>
        </build>
      </profile>

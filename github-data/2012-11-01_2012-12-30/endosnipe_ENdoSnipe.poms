				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-pmd-plugin</artifactId>
				<!-- version>2.5</version -->
				<configuration>
					<sourceEncoding>${src.encoding}</sourceEncoding>
					<targetJdk>${jdk.version}</targetJdk>
					<!--
					<includes>
						<include>**/*foo.java</include>
					</includes>
					<excludes>
						<exclude>**/bar.java</exclude>
					</excludes>
					<excludeRoots>
						<excludeRoot>src/main/java/stubs</excludeRoot>
					</excludeRoots>
					-->
				</configuration>
				<reportSets>
					<reportSet>
						<reports>
							<!-- report>pmd</report -->
							<report>cpd</report>
						</reports>
					</reportSet>
				</reportSets>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>findbugs-maven-plugin</artifactId>
				<!-- version>2.3.1</version -->
				<configuration>
					<!--
					<includeFilterFile>${basedir}/findbugs-include.xml</includeFilterFile>
					<excludeFilterFile>${basedir}/findbugs-exclude.xml</excludeFilterFile>
					-->
					<xmlOutput>true</xmlOutput>
					<failOnError>false</failOnError>
					<!-- High|Normal|Low|Exp|Ignore -->
					<threshold>Normal</threshold>
					<!-- Min|Default|Max -->
					<effort>Default</effort>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>javancss-maven-plugin</artifactId>
				<!-- version>2.0</version -->
				<configuration>
					<lineThreshold>300</lineThreshold>
					<sourceEncoding>${src.encoding}</sourceEncoding>
					<!-- for 2.0-beta3 -->
					<failOnError>false</failOnError>
					<!--
					<includes>
						<include>**/*foo.java</include>
					</includes>
					<excludes>
						<exclude>**/bar.java</exclude>
					</excludes>
					-->
				</configuration>
			</plugin>
			<!--
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-report-plugin</artifactId>
